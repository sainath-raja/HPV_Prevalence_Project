        }
        h2 {
            font-size: 1.875rem;
            font-weight: 600;
            color: #2d3748; /* Slightly lighter dark text */
            margin-top: 2rem;
            margin-bottom: 1.25rem;
            border-bottom: 2px solid #a0aec0; /* Underline for sections */
            padding-bottom: 0.5rem;
        }
        h3 {
            font-size: 1.5rem;
            font-weight: 500;
            color: #4a5568; /* Medium dark text */
            margin-top: 1.5rem;
            margin-bottom: 1rem;
        }
        ul {
            list-style: disc;
            padding-left: 1.5rem;
            margin-bottom: 1rem;
        }
        ul li {
            margin-bottom: 0.5rem;
            color: #4a5568;
            line-height: 1.5;
        }
        table {
            width: 100%;
            border-collapse: collapse;
            margin-bottom: 1.5rem;
            background-color: #f7fafc; /* Light background for tables */
            border-radius: 0.5rem;
            overflow: hidden;
        }
        th, td {
            border: 1px solid #cbd5e0; /* Light gray border */
            padding: 0.75rem;
            text-align: left;
            color: #2d3748;
        }
        th {
            background-color: #ebf4ff; /* Lighter blue for table headers */
            font-weight: 600;
        }
        .formula-container {
            background-color: #e2e8f0; /* Light gray for formula background */
            padding: 1.5rem;
            border-radius: 0.75rem;
            margin-top: 1rem;
            margin-bottom: 1rem;
            display: flex;
            justify-content: center;
            align-items: center;
            font-size: 1.25rem; /* Larger font for formula */
            font-weight: 500;
            color: #2d3748;
            overflow-x: auto; /* Allow horizontal scrolling for long formulas */
        }
        /* Responsive adjustments */
        @media (max-width: 768px) {
            body {
                padding: 1rem;
